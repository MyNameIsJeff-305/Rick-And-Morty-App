---
// import "../styles/global.css";
---

<header
    class="flex flex-col px-4 mx-auto max-w-xl lg:py-8 lg:px-6 items-center"
>
    <section class="flex flex-col items-center mx-auto text-center mb-5 lg:mb-8">
        <picture class="flex w-[80%] justify-center">
            <img
                src="/public/images/Rick_and_Morty.svg.png"
                alt="Rick and Morty Logo"
            />
        </picture>
        <span
            class="font-custom mb-4 text-2xl text-center tracking-tight font-extrabold text-white"
        >
            Wubba Lubba Dub Dub!
        </span>

    </section>
    <section class="flex flex-row">
        
    </section>
</header>
